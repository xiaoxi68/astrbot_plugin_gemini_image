{
  "model_name": {
    "description": "模型名称",
    "type": "string",
    "hint": "如 gemini-2.5-flash-image",
    "default": "gemini-2.5-flash-image"
  },
  "gcli2api_base_url": {
    "description": "gcli2api 基础地址",
    "type": "string",
    "default": "http://127.0.0.1:7861"
  },
  "gcli2api_api_password": {
    "description": "gcli2api API 密码（用于 x-goog-api-key 或 Bearer）",
    "type": "string",
    "default": "pwd"
  },
  "group_control_mode": {
    "description": "群控制模式",
    "type": "string",
    "options": ["off", "whitelist", "blacklist"],
    "default": "off"
  },
  "group_list": {
    "description": "群白/黑名单列表（群号字符串）",
    "type": "list",
    "default": []
  },
  "group_rate_window_seconds": {
    "description": "群调用限流周期（秒）",
    "type": "int",
    "default": 3600
  },
  "group_rate_max_calls": {
    "description": "单群每周期最大调用次数",
    "type": "int",
    "default": 10
  },
  "temperature": {
    "description": "采样温度 (0.0-2.0) 推荐0.7/1.0",
    "type": "float",
    "default": 1.0
  },
  "max_retry_attempts": {
    "description": "每个密钥的最大重试次数",
    "type": "int",
    "default": 3
  },
  "nap_server_address": {
    "description": "Napcat 文件转发主机（可选）",
    "type": "string",
    "default": "localhost"
  },
  "nap_server_port": {
    "description": "Napcat 文件转发端口（可选）",
    "type": "int",
    "default": 3658
  }
}
